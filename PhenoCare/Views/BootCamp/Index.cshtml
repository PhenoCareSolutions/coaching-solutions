@{
    ViewBag.Title = "PhenoCare - Bootcamp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="contact-top-buffer">
    <div class="col-md-offset-1 col-md-10 ">
        <div class="bg-primary well-sm">
            <h1>Bootcamp</h1>
            <p>If you plan to come to bootcamp, get prepared to learn, discuss, and write code!</p>
        </div>
        <hr />
        <div>
            <p class="lead">
                Congratulations freshers!! Phenocare runs free bootcamps every month. Get along with your friends to avail benefits.
            </p>
            <p><span class="label label-info">What is bootcamp?</span> Here phenocare teaches free core Java and core C# to freshers and counsel them for further technology.</p>
        </div>
        <table class="table table-striped">
            <tr>
                <th>Technology
                </th>
                <th>Start Date
                </th>
                <th>Days
                </th>
                <th>Timings
                </th>
                <th></th>
            </tr>
            <tr>
                <td><span class="label label-primary">Free</span> Freshers C# bootcamp</td>
                <td>24-June-14</td>
                <td>Sat - Sun</td>
                <td>9:00 pm - 11.00 pm</td>
                <td>
                    <a href="@Url.Action("Index", "Enrollment", new { course = "C# bootcamp" })" class="btn btn-success">Register
                    </a>
                </td>
            </tr>
            <tr>
                <td><span class="label label-primary">Free</span> Freshers Java bootcamp</td>
                <td>5-July-14</td>
                <td>Sat - Sun</td>
                <td>5.00 pm - 7.00 pm</td>
                <td>
                    <a href="@Url.Action("Index", "Enrollment", new { course = "Java bootcamp" })" class="btn btn-success">Register
                    </a>
                </td>
            </tr>
            <tr>
                <td><span class="label label-primary">Free</span> Freshers C/C++ bootcamp</td>
                <td>1-July-14</td>
                <td>Tue - Sun</td>
                <td>5.15 pm - 7.00 pm</td>
                <td>
                    <a href="@Url.Action("Index", "Enrollment", new { course = "C/C++ bootcamp" })" class="btn btn-success">Register
                    </a>
                </td>
            </tr>
        </table>
    </div>
</div>

@section pageScript
{
    <script type="text/javascript">
        $(document).ready(function () {
            $(".nav li").removeClass("active");
            $("#bootcamp").addClass('active');
        });
    </script>

    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-51105728-1', 'phenocare.com');
        ga('send', 'pageview');

    </script>
}